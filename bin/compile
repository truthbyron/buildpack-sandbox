#!/usr/bin/env bash
# bin/compile <build-dir> <cache-dir>

set -eo pipefail

BUILD_DIR=$1
CACHE_DIR=$2
ENV_DIR=$3

echo "Our arguments are:"
echo "BUILD_DIR=$BUILD_DIR"
echo "CACHE_DIR=$CACHE_DIR"
echo "ENV_DIR=$ENV_DIR"

echo "contents of cache dir:"
ls $CACHE_DIR
find $CACHE_DIR

echo "contents of build dir:"
ls $BUILD_DIR
find $BUILD_DIR
